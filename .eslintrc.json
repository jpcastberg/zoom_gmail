{
    "env": {
        "es2020": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "eol-last": "error",
        "func-style": ["error", "declaration"],
        "indent": ["error", {
            "SwitchCase": 1
        }],
        "no-multiple-empty-lines": ["error", { "max": 1 }],
        "quote-props": "error",
        "quotes": ["error"],
        "semi": "error",
        "space-before-function-paren": ["error", {
            "anonymous": "always",
            "named": "never",
            "asyncArrow": "always"
        }]
    }
}
