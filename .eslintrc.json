{
    "env": {
        "es2020": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "eol-last": "error",
        "no-multiple-empty-lines": ["error", { "max": 1 }],
        "quote-props": "error",
        "quotes": ["error"],
        "semi": "error"
    }
}
